                </a>
            </li><!-- End Dashboard Nav -->
            <li class="nav-item">
                <a class="nav-link " asp-action="Index" asp-controller="TransactionNewsletter">
                    <i class="bi bi-grid"></i>
                    <span>TransactionContactUs</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link " asp-action="Index" asp-controller="TransactionBookTable">
                    <i class="bi bi-grid"></i>
                    <span>Transaction Book Table</span>
                </a>
            </li>

            <li class="nav-heading">Pages</li>

            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterMenu">
                   
                    <span>MasterMenu</span>
                </a>
            </li><!-- End Profile Page Nav -->

            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/FeedBack">

                    <span>FeedBack</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterCategoryMenu">
                    
                    <span>MasterCategoryMenu</span>
                </a>
            </li><!-- End F.A.Q Page Nav -->

            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterContactUsInformation">

                    <span>MasterContactUsInformation</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterItemMenu">

                    <span>Master Item Menu</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterOFfer">

                    <span>Master OFfer</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterService">

                    <span>Master Service</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterSlider">

                    <span>Master Slider</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterPartner">

                    <span>Master Partner</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterWorkingHour">

                    <span>MasterMaster Working Hour</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/MasterSocialMedia">

                    <span>Master Social Media</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link collapsed" href="/Admin/SystemSetting">

                    <span>System Setting</span>
                </a>
            </li>
        </ul>

    </aside><!-- End Sidebar-->
    <main id="main" class="main">
        @RenderBody()
    </main>






