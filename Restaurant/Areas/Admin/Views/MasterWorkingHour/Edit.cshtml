@model Restaurant.Areas.Admin.ViewModels.MasterWorkingHourModel

@{
    ViewData["Title"] = "Edit";
}

<div class="pagetitle">
<h1>Master Working Hour</h1>
<nav>
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="index.html">Home</a></li>
        <li class="breadcrumb-item "><a asp-action="Index" asp-controller="MasterWorkingHour">Master Working Hour</a></li>
        <li class="breadcrumb-item active">Edit</li>
    </ol>
</nav>

</div>
<hr />
<section>
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Multi Columns Form</h5>
                    <form asp-action="Edit" novalidate="" class="row g-3">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <input type="hidden" asp-for="MasterWorkingHourId" />
                        <div class="form-group col-md-6">
                            <label asp-for="MasterWorkingHoursIdName" class="control-label"></label>
                            <input asp-for="MasterWorkingHoursIdName" class="form-control" />
                            <span asp-validation-for="MasterWorkingHoursIdName" class="text-danger"></span>
                        </div>
                        <div class="form-group col-md-6">
                            <label asp-for="MasterWorkingHoursIdTimeFormTo" class="control-label"></label>
                            <input asp-for="MasterWorkingHoursIdTimeFormTo" class="form-control" />
                            <span asp-validation-for="MasterWorkingHoursIdTimeFormTo" class="text-danger"></span>
                        </div>

                        <div class="form-group col-12">
                            <input type="submit" value="Save" class="btn btn-primary" />
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<div>
    <a asp-action="Index">Back to List</a>
</div>

